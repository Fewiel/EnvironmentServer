@using EnvironmentServer.Web.ViewModels.EnvSetup
@model EnvSetupViewModel
@{
    ViewData["Title"] = "Git Informations - Environment Setup";
}
<div class="d-flex align-items-center justify-content-center fullHeight">
    <form method="post" asp-action="PhpVersion">
        <input type="hidden" asp-for="Name" />
        <div class="mb-3">
            <label asp-for="WgetURL" class="form-label">Zip Link</label>
            <input asp-for="WgetURL" class="form-control" />
            <span asp-validation-for="WgetURL"></span>
        </div>
        <button type="submit" class="btn btn-primary">Next</button>
    </form>
</div>